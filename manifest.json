{
  "name": "SSKinopoisk",
  "version": "1.0.3",
  "description": "Watch movies for free",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.kinopoisk.ru/film/*",
        "https://*.kinopoisk.ru/series/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "css": [
        "styles.css"
      ],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
    "https://*.ggkinopoisk.ru/",
    "https://ww1.flink.su/*"
  ],
  "permissions": [
    "tabs"
  ]
}