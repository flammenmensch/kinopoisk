{
  "name": "SSKinopoisk",
  "version": "1.0.0",
  "description": "Watch movies for free",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://*.kinopoisk.ru/film/*"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab",
    "tabs"
  ]
}